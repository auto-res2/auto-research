{
    "experiment_name": "cifar10_resnet",
    "random_seed": 42,
    "data": {
        "dataset": "CIFAR10",
        "batch_size": 128,
        "num_workers": 2,
        "data_dir": "data"
    },
    "model": {
        "name": "resnet18",
        "num_classes": 10
    },
    "training": {
        "epochs": 10,
        "optimizers": {
            "ACM": {
                "lr": 0.001,
                "beta": 0.9,
                "weight_decay": 1e-4
            },
            "Adam": {
                "lr": 0.001,
                "betas": [0.9, 0.999],
                "weight_decay": 1e-4
            }
        },
        "scheduler": {
            "type": "StepLR",
            "step_size": 5,
            "gamma": 0.1
        },
        "save_model": true,
        "save_dir": "models/cifar10"
    },
    "test_mode": false,
    "output_dir": "logs/cifar10"
}
